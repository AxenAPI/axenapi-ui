<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.*?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.demojavafx.OKHLTM"
            prefHeight="1000.0" prefWidth="1000.0"
            stylesheets="@styles.css">

    <top>
        <Label fx:id="label" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="89.0" prefWidth="99.0" text="Well Done!" textAlignment="CENTER" BorderPane.alignment="CENTER">
             <font>
                <Font size="29.0" />
            </font>
         </Label>
   </top>
<center>
                <ImageView fx:id="image"
                layoutX="20.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@paw.png" />
                </image>
            </ImageView>
            </center>
            <bottom>
            <Button fx:id="openSourceCode" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onAction="#openSourceCode" prefHeight="70.0" prefWidth="551.0" text="Open Source Code" BorderPane.alignment="CENTER" />
    </bottom>
</BorderPane>
